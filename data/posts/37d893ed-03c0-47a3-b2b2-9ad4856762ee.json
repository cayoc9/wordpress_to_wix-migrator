                    {
                        "type": "TEXT",
                        "id": "",
                        "nodes": [],
                        "textData": {
                            "text": "1. Prazo Crucial para o Simples Nacional e MEI",
                            "decorations": [
                                {
                                    "type": "BOLD",
                                    "fontWeightValue": 700
                                },
                                {
                                    "type": "ITALIC",
