                    {
                        "type": "TEXT",
                        "id": "",
                        "nodes": [],
                        "textData": {
                            "text": "2. O Que É o Simples Nacional e o MEI?",
                            "decorations": [
                                {
                                    "type": "BOLD",
                                    "fontWeightValue": 700
                                },
                                {
                                    "type": "ITALIC",
                                    "italicData": true
